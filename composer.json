{
  "require": {
    "php": ">=5.3.0",
    "composer/installers": "v1.0.12",
    "webdevstudios/cmb2": "dev-master",
    "jjgrainger/wp-custom-post-type-class": "^1.4"
  },
  "autoload": {
    "files": [
      "vendor/cmb2/init.php",
      "vendor/jjgrainger/wp-custom-post-type-class/src/CPT.php"
    ]
  },
  "extra": {
    "installer-paths": {
      "vendor/{$name}/": [
        "webdevstudios/cmb2"
      ]
    }
  }
}
